statement_one = False

statement_two = True

#IF/AND FUNCTION
def graduation_reqs(gpa, credits):
  if (gpa >= 2.0) and (credits >= 120):
    return "You meet the requirements to graduate!"

#IF/OR FUNCTION
def graduation_mailer(gpa,credits):
  if (gpa>=2.0) or (credits >= 120):
    return True

#IF/NOT FUNCTION
def graduation_mailer(gpa,credits):
  if (gpa>=2.0) or (credits >= 120):
    return True
